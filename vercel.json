{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "Frontend/package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "Frontend/$1",
      "check": true
    },
    {
      "src": "/(.*)",
      "status": 404,
      "dest": "Frontend/index.html"
    }
  ]
}